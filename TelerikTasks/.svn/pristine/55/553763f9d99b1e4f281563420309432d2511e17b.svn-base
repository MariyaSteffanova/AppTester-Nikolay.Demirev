<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Task06.aspx.cs" Inherits="Task06.Task06" %>

<%@ Register Assembly="Telerik.Web.UI" Namespace="Telerik.Web.UI" TagPrefix="telerik" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <asp:TextBox ID="TextBoxScreen" runat="server" ReadOnly="True" Width="300px"></asp:TextBox>
        <br />
        <asp:Table ID="Table1" runat="server" Height="175px" Width="300px">
            <asp:TableRow runat="server">
                <asp:TableCell runat="server" ColumnSpan="2">
                    <asp:Button ID="ButtonBackspace" runat="server" Text="<-" Width="118px" OnClick="ButtonBackspaceClick" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonC" runat="server" Text="C" Width="57px" OnClick="ButtonCClick" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonChangeSign" runat="server" Text="±" Width="57px" OnClick="ButtonChangeSignClick" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonSqrt" runat="server" Text="√" Width="57px" OnClick="ButtonSqrtClick" Height="26" /></asp:TableCell>
            </asp:TableRow>
            <asp:TableRow runat="server">
                <asp:TableCell runat="server">
                    <asp:Button ID="Button7" runat="server" Text="7" Width="57px" OnClick="Button7Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button8" runat="server" Text="8" Width="57px" OnClick="Button8Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button9" runat="server" Text="9" Width="57px" OnClick="Button9Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonDivide" runat="server" Text="/" Width="57px" OnClick="ButtonDivideClick" Height="26" /></asp:TableCell>
                <asp:TableCell ID="TableCell1" runat="server" RowSpan="4">
                    <asp:Button ID="ButtonEquals" runat="server" Text="=" Width="57px" Height="130" OnClick="ButtonEqualsClick" /></asp:TableCell>
            </asp:TableRow>
            <asp:TableRow runat="server">
                <asp:TableCell runat="server">
                    <asp:Button ID="Button4" runat="server" Text="4" Width="57px" OnClick="Button4Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button5" runat="server" Text="5" Width="57px" OnClick="Button5Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button6" runat="server" Text="6" Width="57px" OnClick="Button6Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonMultiply" runat="server" Text="*" Width="57px" OnClick="ButtonMultiplyClick" Height="26" /></asp:TableCell>
            </asp:TableRow>
            <asp:TableRow runat="server">
                <asp:TableCell runat="server">
                    <asp:Button ID="Button1" runat="server" Text="1" Width="57px" OnClick="Button1Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button2" runat="server" Text="2" Width="57px" OnClick="Button2Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="Button3" runat="server" Text="3" Width="57px" OnClick="Button3Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonMinus" runat="server" Text="-" Width="57px" OnClick="ButtonMinusClick" Height="26" /></asp:TableCell>
            </asp:TableRow>
            <asp:TableRow runat="server">
                <asp:TableCell runat="server" ColumnSpan="2">
                    <asp:Button ID="Button0" runat="server" Text="0" Width="118px" OnClick="Button0Click" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonComma" runat="server" Text="," Width="57px" OnClick="ButtonCommaClick" Height="26" /></asp:TableCell>
                <asp:TableCell runat="server">
                    <asp:Button ID="ButtonPlus" runat="server" Text="+" Width="57px" OnClick="ButtonPlusClick" Height="26" /></asp:TableCell>
            </asp:TableRow>
        </asp:Table>
    </div>
    </form>
</body>
</html>
